variant Suit = Spade | Heart | Club | Diamond;
variant CardNum = Ace | N2 | N3 | N4 | N5 | N6 | N7 | N8 | N9 | N10 | Jack | Queen | King;
# Type aliases are not supported yet
variant Card = {CardNum, Suit};

variant Deck = Card[];

variant Player = { @name string };

variant Game = {
  @players Player[],
  @deck Deck
};

variant GameState = {
  @game Game,
  @players Player[],
  @deck Deck
};



