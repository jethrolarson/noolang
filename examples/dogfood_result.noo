# Test user-defined Result ADT

# Define Result type
type Result a b = Ok a | Err b;

# Test with consistent string types
success = Ok "good";
failure = Err "bad";

# Pattern matching
handle_result = fn res => match res with (
  Ok value => value;
  Err msg => msg
);

result1 = handle_result success;
result2 = handle_result failure;

{ @ok_result result1, @err_result result2 }