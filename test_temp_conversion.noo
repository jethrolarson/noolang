# Temperature conversion using |? with division
celsiusToFahrenheit = fn celsius => (
  multiplyBy9 = celsius * 9;
  divideBy5 = fn x => x / 5;
  add32 = fn x => x + 32;
  multiplyBy9 |? divideBy5 |? add32
);

# Test cases  
test1 = celsiusToFahrenheit 0;     # Some 32.0
test2 = celsiusToFahrenheit 100;   # Some 212.0
test2