# Test the extract pattern for record key extraction

# Test with a simple record
math = { @add fn x y => x + y;
         @multiply fn x y => x * y;
         @square fn x => x * x };

# Extract all keys as local definitions
extract math;

# Now we can use the functions directly
result1 = add 2 3;
result2 = multiply 4 5;
result3 = square 6;

# Test with import pattern
import_math = import "std/math";
extract import_math;

# Use imported functions directly
result4 = max 10 20;
result5 = min 5 15;

# Verify all results
{ @result1 result1;
  @result2 result2;
  @result3 result3;
  @result4 result4;
  @result5 result5 } 